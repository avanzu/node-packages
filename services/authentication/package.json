    "main": "dist/index.js",
    "private": true,
    "scripts": {
        "prebuild": "rimraf ./dist",
        "build": "tsc --project tsconfig.build.json",
        "postbuild": "tsc-alias -p tsconfig.build.json",
        "test": "jest",
        "test:integration": "jest --config=jest.integration.config.js",
        "lint": "eslint . ",
        "jest": "jest",
